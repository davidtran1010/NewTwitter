# Uncomment the next line to define a global platform for your project
 platform :ios, '11.0'

target 'NewTwitter' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NewTwitter
  pod 'TwitterKit'
 
  pod 'ObjectMapper', '~> 3.0.0'
  pod 'Alamofire', '~> 4.5'
  pod 'PromiseKit', '~> 4.5'
  pod 'STTwitter'
  pod 'SwiftyJSON'
  pod 'Kingfisher', '~> 4.0'
  pod 'SDWebImage', '~> 4.0'
 pod 'ImageViewer'
  target 'NewTwitterTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NewTwitterUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
